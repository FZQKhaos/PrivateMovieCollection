<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="257.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GUI.Controller.NewMovieWindowController">
   <children>
      <VBox alignment="CENTER_LEFT" layoutX="51.0" layoutY="50.0" prefHeight="142.0" prefWidth="74.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Title">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Category">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="imdb Rating">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="File">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox alignment="CENTER_LEFT" layoutX="126.0" layoutY="50.0" prefHeight="142.0" prefWidth="240.0" scaleShape="false" spacing="5.0">
         <children>
            <TextField fx:id="txtTitle">
               <VBox.margin>
                  <Insets right="25.0" />
               </VBox.margin>
            </TextField>
            <ComboBox fx:id="cbCategory" prefWidth="150.0" />
            <TextField fx:id="txtRating" prefHeight="26.0" prefWidth="208.0" scaleShape="false">
               <VBox.margin>
                  <Insets right="175.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="28.0" prefWidth="186.0" spacing="5.0">
               <children>
                  <TextField fx:id="txtFile" editable="false" />
                  <Button mnemonicParsing="false" onAction="#onActionChoose" text="Choose" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutX="71.0" layoutY="192.0" prefHeight="38.0" prefWidth="240.0" spacing="25.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onActionSave" text="Save" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onActionCancel" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
